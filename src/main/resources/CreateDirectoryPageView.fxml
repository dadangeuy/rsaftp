<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER_LEFT" prefHeight="600.0" prefWidth="800.0"
      spacing="10.0" xmlns="http://javafx.com/javafx/8.0.121">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <Label text="Folder Name :"/>
    <TextField fx:id="folderNameField"/>
    <HBox alignment="CENTER" spacing="10.0">
        <Button fx:id="createButton" mnemonicParsing="false" prefWidth="100.0" text="Create"/>
        <Button fx:id="cancelButton" mnemonicParsing="false" prefWidth="100.0" text="Cancel"/>
    </HBox>
</VBox>
